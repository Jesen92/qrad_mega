<script>

    $('body').on('keydown', 'input, select, textarea', function(e) {
        var self = $(this)
            , form = self.parents('form:eq(0)')
            , focusable
            , next
            ;
        if (e.keyCode == 13) {
            focusable = form.find('input,select').filter(':visible');
            next = focusable.eq(focusable.index(this)+1);
            if (next.length) {
                next.focus();
            } else {
                form.submit();
            }
            return false;
        }
    });

    var suite = "";
    var single_device = 0;
    var multiple_devices = 0;
    var price_single = 0.00;
    var price_multiple = 0.00;

    document.getElementById("submit_contact_button").disabled = true;

    window.onload = setMultiple();
    window.onload = setSingle();
    window.onload = setPaket();

    function enableBtn(){
        document.getElementById("submit_contact_button").disabled = false;
    }

    function setMultiple(){
        var mult = document.getElementById("users_service_multiple_mobile").value;
        document.getElementById("multiple").innerHTML = parseInt(mult);
        multiple_devices = parseInt(mult);
        calculateTotalPrice();
    }

    function setSingle(){
        var sing = document.getElementById("users_service_one_mobile").value;
        document.getElementById("single").innerHTML = parseInt(sing);
        single_device = parseInt(sing);
        calculateTotalPrice();
    }

    function setMultiplePlus(){
        var mult = document.getElementById("users_service_multiple_mobile").value;
        document.getElementById("multiple").innerHTML = parseInt(mult)+1;
        multiple_devices = parseInt(mult)+1;
        calculateTotalPrice();
    }

    function setMultipleMinus(){
        var mult = document.getElementById("users_service_multiple_mobile").value;
        if(parseInt(mult)-1 >= 0){
            document.getElementById("multiple").innerHTML = parseInt(mult)-1;
            multiple_devices = parseInt(mult)-1;
            calculateTotalPrice();
        }
    }

    function setSinglePlus(){
        var sing = document.getElementById("users_service_one_mobile").value;
        document.getElementById("single").innerHTML = parseInt(sing)+1;
        single_device = parseInt(sing)+1;
        calculateTotalPrice();
    }

    function setSingleMinus(){
        var sing = document.getElementById("users_service_one_mobile").value;
        if(parseInt(sing)-1 >= 0){
            document.getElementById("single").innerHTML = parseInt(sing)-1;
            single_device = parseInt(sing)-1;
            calculateTotalPrice();
        }

    }

    function setPaket(){
        var paket = document.getElementById("users_service_package").value;

        if(paket) {
            document.getElementById("paket").innerHTML = paket;
            suite = paket;
        }
        else {
            document.getElementById("paket").innerHTML = "-------";
            suite = "";
        }
        setPriceByPackage();
    }

    function setPriceByPackage(){
      if( suite.indexOf("Essentials") != -1){
        price_single = 2.89;
        price_multiple = 5.79;
      }else if(suite.indexOf("Deluxe") != -1){
        price_single = 4.82;
        price_multiple = 9.65;
      }else if(suite.indexOf("Premier") != -1){
        price_single = 6.03;
        price_multiple = 12.06;
      }else if(suite.indexOf("Enterprise") != -1){
        price_single = 8.68;
        price_multiple = 17.37;
      }else{
        price_single = 0.00;
        price_multiple = 0.00;
      }
      calculateTotalPrice();
    }

    function calculateTotalPrice(){
      var total_price = 0.00;
      total_price = (price_single.toFixed(2)*single_device)+(price_multiple.toFixed(2)*multiple_devices);
      document.getElementById("cijena").innerHTML = total_price.toFixed(2);
    }

  //KALKULATOR SELECT VALUE
