<section class="contact page" id="CONTACT">
  <div class="section_overlay">
    <div class="container">
      <div class="col-md-10 col-md-offset-1 wow bounceIn">
        <!-- Start Contact Section Title-->
        <div class="section_title">
          <h2><%= t(".get_in_touch") %></h2>
          <p><%= t(".contact_text_first") %></br>
          <%= t(".contact_text_second") %></p>
        </div>
      </div>
    </div>

    <div class="contact_form wow bounceIn">
      <div class="container">

        <!-- START ERROR AND SUCCESS MESSAGE -->
        <div class="form_error text-center">
          <div class="name_error hide error"><%= t(".contact_errors.enter_name") %></div>
          <div class="email_error hide error"><%= t(".contact_errors.enter_email") %></div>
          <div class="email_val_error hide error"><%= t(".contact_errors.enter_valid_email") %></div>
          <div class="message_error hide error"><%= t(".contact_errors.enter_message") %></div>
        </div>
        <div class="Sucess"></div>
        <!-- END ERROR AND SUCCESS MESSAGE -->

        <!-- FORM -->
        <%= simple_form_for(:email_form, url: contact_us_services_path ,class: "form-control") do |f|%>
          <div class="row">
            <div class="col-md-4">
              <%= f.input :name,label:false, input_html: {class: "form-control", id: "name"}, required: true, placeholder: t(".contact_us_name") %>
              <%= f.input :email,label:false, input_html: {class: "form-control", id: "email"},required: true , placeholder: t(".contact_us_email")%>
              <%= f.input :subject,label:false, input_html: {class: "form-control", id: "subject"},required: true, placeholder: t(".contact_us_subject") %>
            </div>


            <div class="col-md-8">
              <%= f.input :body ,required: true, label: false, input_html: { id: "message", rows: "25", cols: "10", class: "form-control"}, placeholder: t(".contact_us_body"), :as => :text %>
              <%= f.button :submit, t(".contact_us_send"), class: "btn btn-default submit-btn", id: 'submit_contact_button' %>
            </div>
          </div>
          <div class="row">
            <div class="col-md-4">
              <% unless flash[:recaptcha_error].nil? %>
                <div class="alert alert-danger" role="alert"><%= flash[:recaptcha_error] %></div>
              <% end %>
              <%= recaptcha_tags callback: 'enableBtn' %>
            </div>
          </div>
        <% end %>
        <!-- END FORM -->
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="col-md-12 wow bounceInLeft">
          <div class="social_icons">
            <ul>
              <li><a href="https://www.facebook.com/mprdoo" target="_blank"><i class="fa fa-facebook"></i></a>
              </li>
              <li><a href="https://www.linkedin.com/company/megatrend" target="_blank"><i class="fa fa-linkedin"></i></a></li>

            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- END CONTACT -->


